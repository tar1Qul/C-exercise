#include<stdio.h>

int main(){
int n, n1=3, n2=3;
n=n1+n2;
int index=0;
int a[n1];
int b[n2];
int c[n];

for(int i=0; i<n1; i++){
    printf("enter a n1 array number");
    scanf("%d",&a[i]);
}
for(int i=0; i<n2; i++){
    printf("enter a n2 array number");
    scanf("%d",&b[i]);
}
for(int i=0; i<n1; i++){
    c[index]=a[i];
    index++;
}
for(int i=0; i<n2; i++){
    c[index]=b[i];
    index++;
}
for(int i=0; i<n; i++){
    printf("%d\n",c[i]);
}
return 0;

}
