#include<stdio.h>
int main(){

int num;

printf("enter a number: ");
scanf("%d",&num);

int count=0;
int temp=num;
while(1) {

    temp=temp/10;
    count++;if(temp==0){
    break;
    }
}

int original,reverse=0,remiainder;

original=num;
while(num!=0){
     remiainder = num%10;
     reverse= reverse *10 +remiainder;
     num/=10;
     if (original==remiainder){
       printf("%d is a palindrome number.\n",original);
     }
     else {
        printf("%d is not a palindrome number.\n",original);
     }
}


return 0;
}

