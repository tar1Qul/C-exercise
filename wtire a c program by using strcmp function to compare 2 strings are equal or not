#include <stdio.h>
#include <string.h>

int main() {
    char str1[1000];
    char str2[1000];

    printf("Enter the first string: ");
    gets(str1);

    printf("Enter the second string: ");
    gets(str2);

    int d = strcmp(str1, str2);

    if(d ==0 ){

          printf("\nStrings are equal. ");
    }

    else{

          printf("Strings are not equal. ");
        }


    return 0;
}
