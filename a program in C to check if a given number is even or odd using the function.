
#include <stdio.h>

void checknumber(int number){
    if (number % 2 == 0){
        printf("%d is even number.\n",number);
    }
    else{
        printf("%d is odd number.\n",number);
    }
}
int main() {
    int number,result;
    printf("Enter a number: ");
    scanf("%d", &number);
    
    checknumber(number);

    return 0;
}
