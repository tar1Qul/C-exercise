#include <stdio.h>

int factorial(int number) {
    int result = 1;
    for (int i = 1; i <= number; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int number;
    printf("Enter a number to calculate factorial: ");
    scanf("%d", &number);

    int result = factorial(number);

    printf("Factorial: %d\n", result);

    return 0;
}
