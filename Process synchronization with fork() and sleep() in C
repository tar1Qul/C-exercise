#include <stdio.h>
#include<sys/types.h>
#include<unistd.h>

int main() {
    
    printf("This is line 1\n\n");
    
    int id;
    int i;
    id= fork();
    if(id==0)
    {
        for(i=0;i<5;i++)
        printf("%d",i);
    }
    else if(id>0){
        sleep(2);
        for(i=6;i<=10;i++)
        printf("%d",i);
    }

    return 0;
}
