#include <stdio.h>
int main()
{
    int num, sum = 0;

    do
    {
        printf("Enter a positive integer (enter a negative integer to stop): ");
        scanf("%d", &num);

        if (num >= 0)
        {
            sum += num;
        }
    }
    while (num >= 0);

    printf("The sum of positive integers entered is: %d\n", sum);

    return 0;
}
